#!/system/bin/bash
# load the chroot environment

# Bare minimum of assumptions to get started.
export crLocation="outside"
crManagerHome="/sdcard/.cr"

# Bring in the libraries.
. $crManagerHome/lib/display.sh
. $crManagerHome/lib/mounts.sh
. $crManagerHome/lib/vnc.sh
. $crManagerHome/lib/hacks.sh
. $crManagerHome/lib/config.sh
. $crManagerHome/lib/convenience.sh
. $crManagerHome/lib/core.sh

# Start stuff up.
applyAllHacks
chooseImage "$1" "$2" "$3"
 