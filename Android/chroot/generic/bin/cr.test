#!/bin/bash
# load the chroot environment

# bring in the libraries
. /sdcard/.cr/lib/mounts.sh
. /sdcard/.cr/lib/vnc.sh
. /sdcard/.cr/lib/hacks.sh

# bring in config.
configLocation="/sdcard/.cr/config.d/config"
if [ -e "$configLocation" ]; then
  . /"$configLocation"
else
  echo "Could not find \"$configLocation\". Please see configExample for how to set this up."
  exit 0
fi


# Start stuff up.
applyAllHacks
. /sdcard/.cr/lib/core.sh
