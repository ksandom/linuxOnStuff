#!/bin/bash
# load the chroot environment

crManagerHome="/sdcard/.cr"

# bring in the libraries
. $crManagerHome/lib/mounts.sh
. $crManagerHome/lib/vnc.sh
. $crManagerHome/lib/hacks.sh

# bring in config.
configLocation="$crManagerHome/config.d/config"
if [ -e "$configLocation" ]; then
  . /"$configLocation"
else
  echo "Could not find \"$configLocation\". Please see configExample for how to set this up."
  exit 0
fi


# Start stuff up.
applyAllHacks
. $crManagerHome/lib/core.sh
